import tensorflow as tf

# Define two vectors as lists
vector1 = [1.0, 2.0, 3.0]
vector2 = [4.0, 5.0, 6.0]

# Convert the lists to TensorFlow tensors
tf_vector1 = tf.constant(vector1)
tf_vector2 = tf.constant(vector2)

# Perform element-wise addition
result_vector = tf.add(tf_vector1, tf_vector2)

# Print the result
print("Vector1:", vector1)
print("Vector2:", vector2)
print("Result:", result_vector.numpy())
